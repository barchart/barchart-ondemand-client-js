{
  "name": "@barchart/ondemand-client-js",
  "version": "3.0.0",
  "description": "Barchart OnDemand client for Javascript",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/barchart/barchart-ondemand-client-js"
  },
  "author": "mike@barchart.com",
  "license": "MIT",
  "homepage": "https://github.com/barchart/barchart-ondemand-client-js",
  "bugs": {
    "url": "https://github.com/barchart/barchart-ondemand-client-js/issues"
  },
  "main": "dist/ondemand-client-node.cjs",
  "module": "dist/ondemand-client-node.js",
  "browser": "dist/ondemand-client-browser.js",
  "types": "dist/ondemand-client.d.ts",
  "files": [
    "dist",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "run-p build:** -l",
    "build:browser": "esbuild src/ondemand-client.ts --outfile=dist/ondemand-client-browser.js --platform=browser --bundle --target=es2020 --format=esm",
    "build:node": "tsup --entry.ondemand-client-node src/ondemand-client.ts --format esm,cjs --platform=node --target=es2020",
    "build:types": "tsc --outDir dist",
    "generate": "node devtools/generate-interfaces-from-docs.js"
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  }
}
